{

    "version":2,
    "builds": [
        {
          "src": "./server.js",
          "use": "@vercel/node"
        }
      ],
    
    "routes":[
        {
            "src":"/(.*)",
            "destination":"/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "https://attendify-website-mern-stack.vercel.app"
            }

        }
    ]

}
